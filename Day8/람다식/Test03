class WorkThread implements Runnable{


    @Override
    public void run() {
        //1~100 출력
        for(int i = 1; i <= 100; i++){
            System.out.print(i + " ");
        }
    }
}

public class Main {
    public static void main(String[] args) {
        //정통 스타일
        WorkThread wt = new WorkThread();
        Thread t = new Thread(wt);
        t.start();
        try {
            t.join();//t출력 후 t2실행
        } catch (InterruptedException e) {
            throw new RuntimeException(e);
        }
        System.out.println();
        //람다 스타일
        Thread t2 = new Thread(()->{
            for(int i = 1; i <= 100; i++)
                System.out.print(i + " ");
        });
        t2.start();
    }
}
