class Member// class에 final 테스트
{
    final public void run(){// 1. 메소드에 final을 붙이면 재정의 불가
    }
}
class VIP extends Member // 2. 상속을 못하게 만드는 키워드
{
    // 멤버 선언
    public final int number = 100;  // 3. 변수에 final 붙이면 상수가 되어서 변경 불가!!
    // number = 200;(X)
    public void test(){
        // number = 200; // Error
    }

}
//TIP 코드를 <b>실행</b>하려면 <shortcut actionId="Run"/>을(를) 누르거나
// 에디터 여백에 있는 <icon src="AllIcons.Actions.Execute"/> 아이콘을 클릭하세요.
public class Main {
    public static void main(String[] args) {
        Member member = new Member();
    }
}
