//5x5달팽이 배열 만들기
import java.lang.Math;

public class Main {
    public static void main(String[] args) {
        int[][] arr = new int[5][5];
        int num = 1;
        int limit = 0;
        int i = 0;
        int j = 0;
        int n = (int)(Math.sqrt(25)-1)/2;

        while(num < 26) {
            if(arr[i][j + 1] == 0){//우
                for(j = 0 + limit ; j <= 3 - limit ; j++){
                    arr[i][j] = num;
                    num++;
                }
            }
            //System.out.println("arr[" + i + "][" + j + "] = " + arr[i][j]);
            if(arr[i + 1][j] == 0){//아래
                for(i = 0 + limit ; i <= 3 - limit ; i ++) {
                    //System.out.println("입력숫자 : " + num);
                    arr[i][j] = num;
                    num++;
                }
            }
            //System.out.println("arr[" + i + "][" + j + "] = " + arr[i][j]);
            if(arr[i][j - 1] == 0){//좌
                for(j = 4 - limit ; j >= 1 + limit ; j--){
                    //System.out.println("입력숫자 : " + num);
                    arr[i][j] = num;
                    num++;
                }
            }
            //System.out.println("arr[" + i + "][" + j + "] = " + arr[i][j]);
            if(arr[i - 1][j] == 0){//위
                for(i = 4 - limit ; i >= 1 + limit ; i--){
                    //System.out.println("입력숫자 : " + num);
                    arr[i][j] = num;
                    num++;
                }
                limit++;
                i ++;
                j ++;

            }
            if(limit == n){
                i = limit;
                j = limit;
                arr[i][j] = num;
                num++;
            }
//            else{//마지막
//                i = limit;
//                j = limit;
//                arr[i][j] = num;
//                num++;
//            }
        }//while() 끝

        //System.out.println((Math.sqrt(25)-1)/2);
        num = 0;
        for(i = 0; i < 5; i++){
            for(j = 0; j < 5; j++){
                System.out.print(arr[i][j]);
            }
            System.out.println();
        }
    }
}
