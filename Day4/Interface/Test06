interface Vehicle{
    void run();
}

class Bus implements Vehicle{
    //alt+enter
    @Override
    public void run() {
        System.out.println("버스가 달립니다.");
    }
}
class Taxi implements Vehicle{
    //alt+enter
    @Override
    public void run() {
        System.out.println("택시가 달립니다..");
    }
}
class Subway implements Vehicle{
    //alt+enter
    @Override
    public void run() {
        System.out.println("지하철이 달립니다.");
    }
}
class Driver{

}

public class Main {
    public static void main(String[] args) {
        Bus b = new Bus();
        b.run();
        Taxi t = new Taxi();
        t.run();
        Subway s = new Subway();
        s.run();
    }
}
